{
  "name": "@tjk/sentry-vue3",
  "version": "6.2.3-alpha.7",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tjk/sentry-vue3.git"
  },
  "files": [
    "dist/*",
    "esm/*"
  ],
  "homepage": "https://github.com/tjk/sentry-vue3",
  "author": "Sentry, TJ Koblentz",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@sentry/browser": "6.2.3",
    "@sentry/types": "6.2.3"
  },
  "peerDependencies": {
    "vue": "3.x",
    "vue-router": "4.x"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2",
    "release-it": "^14.10.0",
    "typescript": "^4.3.5",
    "vue": "^3",
    "vue-router": "^4"
  },
  "scripts": {
    "build": "run-p build:es5 build:esm",
    "build:es5": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "postbuild": "find build dist esm -type f | xargs sed -i \"s/process.env.PKG_VERSION/'$(node -p -e \"require('./package.json').version\")'/g\""
  },
  "description": "@sentry/vue ported to vue 3 (EXPERIMENTAL: Use at own risk!)",
  "bugs": {
    "url": "https://github.com/tjk/sentry-vue3/issues"
  }
}
